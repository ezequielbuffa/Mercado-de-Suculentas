{
  "name": "webpage",
  "version": "1.0.0",
  "description": "Nuestro primer projecto de Node js",
  "main": "index.html",
  "scripts": {
    "dev": "lite-server",
    "start": "concurrently \"npm run scss\" \"npm run dev \"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "scss": "node-sass -w css/styles.scss css/styles.css",
    "watch:scss": "onchange 'css/*.scss' -- npm run scss",
    "clean": "rimraf dist",
    "imagemin": "imagemin img/* --out-dir dist/img",
    "usemin": "usemin index.html -d dist --htmlmin -o dist/index.html && usemin about.html -d dist --htmlmin -o dist/about.html && usemin contact.html -d dist --htmlmin -o dist/contact.html && usemin price.html -d dist --htmlmin -o dist/price.html && usemin terms.html -d dist --htmlmin -o dist/terms.html",
    "build": "npm run clean && npm run scss && npm run imagemin && npm run usemin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ezequielbuffa/hotels-guide.git"
  },
  "author": "Ezequiel Buffa",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ezequielbuffa/hotels-guide/issues"
  },
  "homepage": "https://github.com/ezequielbuffa/hotels-guide#readme",
  "devDependencies": {
    "cssmin": "^0.4.3",
    "grunt": "^1.5.3",
    "grunt-contrib-sass": "^2.0.0",
    "htmlmin": "^0.0.7",
    "lite-server": "^2.6.1",
    "node-sass": "^7.0.3",
    "onchange": "^7.1.0",
    "rimraf": "^3.0.2",
    "uglifyjs": "^2.4.11",
    "usemin-cli": "^0.6.0"
  },
  "dependencies": {
    "bootstrap": "^5.2.1",
    "concurrently": "^7.4.0",
    "imagemin": "^8.0.1",
    "jquery": "^3.6.1",
    "open-iconic": "^1.1.1",
    "popper.js": "^1.16.1",
    "uglify-js": "^3.17.3"
  }
}
